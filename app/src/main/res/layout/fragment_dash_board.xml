<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sirtts.hcp.DashBoardActivityFragment"
    tools:showIn="@layout/activity_dash_board">


    <Button
        android:id="@+id/dashboard_logoutbtnid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="369dp"
        android:text="LogOut"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dashboard_vitalbtnid" />

    <Button
        android:id="@+id/dashboard_vitalbtnid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="34dp"
        android:text="Vital Signs"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/dashboard_periodbtnid"
        android:layout_marginRight="8dp" />

    <Button
        android:id="@+id/dashboard_periodbtnid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Period"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="34dp"
        android:layout_marginRight="59dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/dashboard_sugarbtnid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="61dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="97dp"
        android:text="Diabetes"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dashboard_vitalbtnid" />
</android.support.constraint.ConstraintLayout>
